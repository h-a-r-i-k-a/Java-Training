
<xsd:schema xmlns="http://www.w3.org/2001/XMLSchema">
<xsd:element name="EMPS" type="complexTypeEMPS"/>
<xsd:complexType name="complexTypeEMPS">
<xsd:sequence>
<xsd:element name="EMP" type="complexTypeEMP" minOccurs="1" maxOccurs="unbounded"/>
</xsd:sequence>
</xsd:complexType>
<xsd:complexType name="complexTypeEMP">
<xsd:sequence>
<xsd:element name="PRODUCTNAME" type="xsd:string"/>
<xsd:element name="DESCRIPTION" type="xsd:string"/>
<xsd:element name="PRICE" type="xsd:positiveInteger"/>
<xsd:element name="QUANTITY" type="xsd:nonNegativeInteger"/>
</xsd:sequence>
<xsd:attribute name="PRODID" type="xsd:string" use="required"/>
<xsd:attribute name="CATEGORY" type="xsd:string"/>
</xsd:complexType>
</xsd:schema>

