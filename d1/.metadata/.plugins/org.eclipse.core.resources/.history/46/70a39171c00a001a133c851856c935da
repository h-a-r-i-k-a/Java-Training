package com.hcl.generics;

class Data<T>{
    public void swap(T a,T b){
        T t;
        t=a;
        a=b;
        b=t;
        System.out.println("a value"+a+"b value"+b);
    }
}
public class GenDemo {
    public static void main(String[] args) {
        Data obj1=new Data();
        int a=5,b=7;
        obj1.swap(a, b);
    }
    

}
